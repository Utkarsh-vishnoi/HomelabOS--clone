---
version: '3'

networks:
  traefik_network:
    external:
      name: homelabos_traefik

services:
  grocy:
    image: ghcr.io/linuxserver/grocy
    restart: unless-stopped
    environment:
      - TZ={{ common_timezone }}
    networks:
      - traefik_network
    volumes:
      - "{{ volumes_root }}/grocy/config:/config"
{% include './labels.j2' %}
